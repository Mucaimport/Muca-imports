					<section  class="elementor-section elementor-top-section elementor-element elementor-element-35c7f2e elementor-section-boxed elementor-section-height-default elementor-section-height-default" data-id="35c7f2e" data-element_type="section">
						<div class="elementor-container elementor-column-gap-default">
					<div class="elementor-column elementor-col-50 elementor-top-column elementor-element elementor-element-e18858d" data-id="e18858d" data-element_type="column">
			<div class="elementor-widget-wrap elementor-element-populated">
				<div class="elementor-element elementor-element-13433bb e-flex e-con-boxed e-con e-parent" data-id="13433bb" data-element_type="container">
					<div class="e-con-inner">
				<div class="elementor-element elementor-element-f7bcabd elementor-widget elementor-widget-image" data-id="f7bcabd" data-element_type="widget" data-widget_type="image.default">
				<div class="elementor-widget-container">
																<a href="https://lejour.com.br/?source=blog&#038;utm_source=BlogLejour&#038;utm_medium=banner&#038;utm_campaign=banner-jun2025">
							<img  width="1600" height="125" src="https://lejour.com.br/wp-content/uploads/2025/05/Casamentos.com_.br-Banner-320-x-50-640-x-50-px.gif" class="attachment-full size-full wp-image-58139" alt="" />								</a>
