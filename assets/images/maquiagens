			<script>
				const lazyloadRunObserver = () => {
					const lazyloadBackgrounds = document.querySelectorAll( `.e-con.e-parent:not(.e-lazyloaded)` );
					const lazyloadBackgroundObserver = new IntersectionObserver( ( entries ) => {
						entries.forEach( ( entry ) => {
							if ( entry.isIntersecting ) {
								let lazyloadBackground = entry.target;
								if( lazyloadBackground ) {
									lazyloadBackground.classList.add( 'e-lazyloaded' );
								}
								lazyloadBackgroundObserver.unobserve( entry.target );
							}
						});
					}, { rootMargin: '200px 0px 200px 0px' } );
					lazyloadBackgrounds.forEach( ( lazyloadBackground ) => {
						lazyloadBackgroundObserver.observe( lazyloadBackground );
					} );
				};
				const events = [
					'DOMContentLoaded',
					'elementor/lazyload/observe',
				];
				events.forEach( ( event ) => {
					document.addEventListener( event, lazyloadRunObserver );
				} );
			</script>
			<script>var isBlog = window.location.href.includes('/blog');

function setCookie(name, value, days) {
  const date = new Date();
  date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
  const expires = "expires=" + date.toUTCString();
  document.cookie = name + "=" + value + ";" + expires + ";path=/";
}

function getCookie(name) {
  const decodedCookie = decodeURIComponent(document.cookie);
  const cookies = decodedCookie.split(';');
  for(let i = 0; i < cookies.length; i++) {
    let cookie = cookies[i];
    while (cookie.charAt(0) === ' ') {
      cookie = cookie.substring(1);
    }
    if (cookie.indexOf(name + "=") === 0) {
      return cookie.substring(name.length + 1, cookie.length);
    }
  }
  return "";
}

function createTimedModal() {
	if (getCookie("alreadyShownTimedModal") !== "true") {
	  const modalHTML = `
		<div id="timed-modal" class="exit-modal">
		  <div class="timed-modal-content">
			<span class="close-btn" onclick="closeModal()">&times;</span>
			<div class="modal-left">
			  <h4 style="color: #5aa59e;">LEJOUR + FAST SHOP</h4>
			  <h2>Crie grátis seu site e lista de casamento!</h2>
				<div>
					<p>- Site de casamento grátis</p>
					<p>- Lista de presentes em dinheiro</p>
					<p>- Confirmação de presença online</p>
				</div>
			  <button class="modal-action" onclick="handleOffer()">CRIAR AGORA</button>
			</div>
			<div class="modal-right">
			  <img src="https://editor-upload-cdn.optimonk.com/userImages/224486/65f3479e576fef00235f78a5/WhatsApp%20Image%2020_1710443379399.jpg" alt="Imagem" />
			</div>
		  </div>
		</div>
	  `;

	  // Injetando o HTML no final do corpo do documento
	  document.body.insertAdjacentHTML("beforeend", modalHTML);
	  setCookie("alreadyShownTimedModal", "true", 30);
	}
}

function showTimedModal() {
  const modal = document.getElementById("timed-modal");

  if (!modal) {
    createTimedModal();
  }
	
  document.getElementById("timed-modal").style.display = "flex";
}

function createExitIntentModal() {
		  const modalHTML = `
			<div id="exit-intent-modal" class="exit-modal">
			  <div class="modal-content">
				<span class="close-btn" onclick="closeModal()">&times;</span>
				<div class="modal-left">
				  <h4>LEJOUR + FAST SHOP</h4>
				  <h2>Última oportunidade pra criar sua lista com taxa reduzida!</h2>
				  <p>Crie seu site e lista de casamento com a melhor taxa do mercado: de <strong>3,89% por 3,49%</strong>.</p>
				  <button class="modal-action" onclick="handleOffer()">APROVEITAR TAXA REDUZIDA!</button>
				</div>
				<div class="modal-right">
				  <img src="https://lejour.com.br/wp-content/uploads/2024/12/Captura-de-Tela-2024-12-02-às-10.24.12.png" alt="" />
				</div>
			  </div>
			</div>
		  `;

		  // Injetando o HTML no final do corpo do documento
		  document.body.insertAdjacentHTML("beforeend", modalHTML);
}

function showExitIntentModal() {
  const modal = document.getElementById("exit-intent-modal");
	
	if (sessionStorage.getItem("exitIntentShown") === "true") {
   		return; // Se já foi exibido, não faça nada
	}
	
  if (!modal) {
    createExitIntentModal();
  }
	
  document.getElementById("exit-intent-modal").style.display = "flex";
	  sessionStorage.setItem("exitIntentShown", "true");
}

function closeModal() {
	const modal = document.getElementById("exit-intent-modal");
	
	if (modal) {
		modal.style.display = "none";
	}
	
	const timedModal = document.getElementById("timed-modal");
	
	if (timedModal) {
		timedModal.style.display = "none";
	}
}

function handleOffer() {
	if (isBlog) {
		window.open('https://admin-site.lejour.com.br/onboarding/inicio?source=blog&utm_source=BlogLejour&utm_medium=pop-up&utm_campaign=nov24', '_blank')	
	} else {
		window.open('https://admin-site.lejour.com.br/onboarding/inicio?affiliate=homelejour&source=blog&utm_source=BlogLejour&utm_medium=pop-up&utm_campaign=exit_intent_home_nov24', '_blank')	
	}
	
	closeModal();
}

const isHome = window.location.href === 'https://lejour.com.br/';

// if (isHome) {
// 	document.addEventListener("mouseout", function (event) {
// 		if (event.clientY < 0) {
// 			showExitIntentModal();
// 		}
// 	});
// }

if (isBlog) {
	setTimeout(showTimedModal, 30000);
}

